MACRO ASSEMBLER A51 V8.02b
OBJECT MODULE PLACED IN main.OBJ
ASSEMBLER INVOKED BY: C:\Keil\C51\BIN\A51.EXE main.a51 SET(SMALL) DEBUG EP

LOC  OBJ            LINE     SOURCE

                       1     ;------------------------------------------------------
                       2     ; Simple buzzer program for 8051
                       3     ; Buzzer connected to P2.5
                       4     ;------------------------------------------------------
                       5     
0000                   6     ORG 00H          ; Program start address
                       7     
0000 D2A5              8     START:  SETB P2.5          ; Set pin high
0002 110A              9             ACALL DELAY        ; Call delay
0004 C2A5             10             CLR P2.5           ; Set pin low
0006 110A             11             ACALL DELAY        ; Call delay
0008 80F6             12             SJMP START         ; Repeat forever
                      13     
                      14     ;------------------------------------------------------
                      15     ; Delay routine (adjust for tone frequency)
                      16     ;------------------------------------------------------
000A 7A1F             17     DELAY:  MOV R2, #31     ; Outer loop counter
000C 791E             18     D1:     MOV R1, #30     ; Inner loop counter
000E D9FE             19     D2:     DJNZ R1, D2        ; Decrement inner loop
0010 DAFA             20             DJNZ R2, D1        ; Decrement outer loop
0012 22               21             RET
                      22     
                      23             END
